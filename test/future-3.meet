open func sum (a: int, b: int) -> func () -> (int, int) {
    var a = a + 2
    var b = b + 2

    return func () -> (int, int) {
        return (a, b)
    }
}

var a = sum (12, 32)

println a.0 // 12
println a.1 // 32
