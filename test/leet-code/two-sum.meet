
// https://leetcode-cn.com/problems/two-sum/

func twoSum (arr: list<int>, target: int) -> list<int> {
    var mut result: list<int> = []
    
    for i, v : arr {
        var mut k = i + 1
        
        while k < arr.size {
            if v + arr.at(k) == target {
                result.insert(i);
                result.insert(k);
            }
            k += 1
        }
    }
    
    return result
}

println twoSum ([2, 7, 11, 15], 9) // [0, 1]
