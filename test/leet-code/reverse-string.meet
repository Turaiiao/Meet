
// https://leetcode-cn.com/problems/reverse-string/

fun reverseString (arr: list<string>) -> list<string> {
    var k = arr.length - 1, i = 0

    while i < arr.length / 2 {
        arr.swap(i, arr[k])

        i += 1
        k -= 1
    }

    return arr
}

println reverseString (['h', 'e', 'l', 'l', 'o']) // ['0', 'l', 'l', 'e', 'h']