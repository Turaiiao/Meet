
// https://leetcode-cn.com/problems/search-insert-position/

fun searchInsert (nums: list<int>, target: int) -> int {
    var position = -1

    for i, v : nums -> if v == target | v > target {
            position = i

            break
        } elif a < target -> continue

    if position = -1 ->
        position = nums.size()

    return position
}