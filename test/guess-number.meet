
var a = input('Please enter the number to guess: ')
var b = input('Ok, Please input your number: ')

if type(b) != int {
    println 'Please input a number !'
  
    exit(64)
}

loop {
    println match {
        a > b -> 'max'
        
        a < b -> 'min'
        
        a == b {
            break
            
            'win'
        }
    }
}
